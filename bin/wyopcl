#!/bin/bash
#!/bin/sh

##################
# CONFIGURATION
##################

DIR=`dirname "$0"`/..
LIBDIR=$DIR/lib
LIBS="wyc wyil wybs wycs wyrl wyopcl"

. $DIR/bin/wy_common.bash

######################
# RUN APPLICATION
######################
java -server -Xmx128M -cp "$WHILEY_CLASSPATH" wyopcl.WyopclMain -bp "$WHILEY_BOOTPATH" "$@"
